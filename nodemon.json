{
  "restartable": "rs",
  "ignore": [".git", ".gitignore", "node_modules/**/node_modules"],
  "verbose": false,
  "execMap": {
    "js": "node"
  },
  "watch": [
    "./config",
    "./controllers",
    "./models",
    "./routes",
    "./schemas",
    ".env",
    "index.js"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js,json",
  "delay": 800
}
